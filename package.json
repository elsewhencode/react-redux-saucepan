{
  "name": "react-redux-saucepan",
  "engines": {
    "node": "8",
    "npm": "6"
  },
  "version": "1.0.0",
  "description": "Elsewhen's universal react redux starter kit",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./node_modules && npm cache verify && rm ./package-lock.json",
    "flow": " flow './src' && flow-coverage-report -i 'src/**/*.js' -x 'src/test/**'",
    "start:dev": "rm -rf ./dist/* && babel-watch ./src/server/index.dev.js -w ./server",
    "start": "node dist/index.js",
    "prestart": "npm run test && npm run build",
    "test": "echo \"Hmmm... no test specified\" ",
    "build": "webpack --mode production  --config ./scripts/webpack.server.js && webpack --mode production --config ./scripts/webpack.client.js",
    "lint": "eslint './src/**/*.js' && stylelint './src/**/*.js'"
  },
  "author": "Elsewhen Limited <vahid@elsewhen.co>",
  "license": "MIT",
  "repository": "https://github.com/elsewhencode/react-redux-saucepan",
  "dependencies": {
    "axios": "0.18.0",
    "compression": "1.7.2",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "4.1.3",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "4.0.0",
    "redux-thunk": "2.2.0",
    "styled-components": "3.2.6"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "babel-watch": "2.0.7",
    "compression-webpack-plugin": "1.1.11",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.8.2",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "flow-bin": "0.72.0",
    "flow-coverage-report": "0.5.0",
    "flow-webpack-plugin": "1.2.0",
    "prettier-eslint": "8.8.1",
    "style-loader": "0.21.0",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.5.0",
    "stylelint-processor-styled-components": "1.3.1",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
